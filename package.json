{
  "name": "luke-test-v8",
  "version": "0.0.1-SNAPSHOT",
  "license": "SEE LICENSE IN ./LICENSE",
  "description": "Paystack boilerplate",
  "main": "src/app.ts",
  "scripts": {
    "db.migrate": "yarn nps db.migrate",
    "db.revert": "yarn nps db.revert",
    "db.drop": "yarn nps db.drop",
    "db.seed": "yarn nps db.seed",
    "nps": "cross-env NODE_ENV=development nps",
    "serve": "cross-env NODE_ENV=development nps serve",
    "test": "cross-env NODE_ENV=test nps test",
    "testAndWatch": "cross-env NODE_ENV=test nps 'test --watchAll'",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "build": "nps build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate"
  ],
  "dependencies": {
    "@paystackhq/pkg-form-builder": "^0.1.36",
    "@paystackhq/pkg-moc-types": "^0.1.40",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "aws-sdk": "^2.743.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.1.0",
    "dd-trace": "^3.2.0",
    "dotenv": "16.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "glob": "^8.0.3",
    "hot-shots": "^9.2.0",
    "jest": "^29.0.1",
    "jsonfile": "^6.1.0",
    "microframework-w3tec": ">=0.6.3",
    "mongodb": "^3.6.0",
    "mongodb-memory-server": ">=7.0.0",
    "morgan": "^1.9.0",
    "nodemon": "^2.0.19",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "prettier": "^2.5.1",
    "prom-client": "^13.1.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "typedi": "0.8.0",
    "typeorm": "^0.3.9",
    "typescript": "^4.8.3",
    "winston": "3.8.2",
    "yarn": "^1.22.19"
  },
  "resolutions": {
    "**/event-stream": ">=4.0.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "rootDir": ".",
    "testRegex": "./test/.*test.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testTimeout": 60000,
    "testEnvironment": "node",
    "globalSetup": "./src/utils/test/setup.ts",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "automock": false
  },
  "devDependencies": {
    "@types/bcrypt": ">=2.0.0",
    "@types/bluebird": ">=3.5.18",
    "@types/cors": ">=2.8.1",
    "@types/express": ">=4.0.39",
    "@types/figlet": ">=1.2.0",
    "@types/jest": "^29.0.1",
    "@types/morgan": ">=1.7.35",
    "@types/node": "^14.18.28",
    "@types/supertest": ">=2.0.4",
    "@types/uuid": ">=8.3.4",
    "eslint": ">=7.24.0",
    "eslint-plugin-import": ">=2.22.1",
    "eslint-plugin-jsdoc": ">=32.3.0",
    "eslint-plugin-no-null": ">=1.0.2",
    "eslint-plugin-prefer-arrow": ">=1.2.3",
    "cross-env": ">=5.1.1",
    "jest": "^29.0.1",
    "mock-express-request": ">=0.2.0",
    "mock-express-response": ">=0.2.1",
    "ncp": ">=2.0.0",
    "nock": "10.0.0",
    "mongodb-memory-server": ">=7.0.0",
    "ts-jest": "^29.0.0"
  }
}
